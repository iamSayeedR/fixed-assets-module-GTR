{
    "databaseChangeLog": [
        {
            "changeSet": {
                "id": "061-create-equity-transactions-table",
                "author": "system",
                "changes": [
                    {
                        "createTable": {
                            "tableName": "equity_transactions",
                            "columns": [
                                {
                                    "column": {
                                        "name": "transaction_id",
                                        "type": "BIGINT",
                                        "autoIncrement": true,
                                        "constraints": {
                                            "primaryKey": true,
                                            "nullable": false
                                        }
                                    }
                                },
                                {
                                    "column": {
                                        "name": "transaction_number",
                                        "type": "VARCHAR(50)",
                                        "constraints": {
                                            "unique": true
                                        }
                                    }
                                },
                                {
                                    "column": {
                                        "name": "transaction_date",
                                        "type": "DATE",
                                        "constraints": {
                                            "nullable": false
                                        }
                                    }
                                },
                                {
                                    "column": {
                                        "name": "equity_account_id",
                                        "type": "BIGINT",
                                        "constraints": {
                                            "nullable": false
                                        }
                                    }
                                },
                                {
                                    "column": {
                                        "name": "transaction_type",
                                        "type": "VARCHAR(50)",
                                        "constraints": {
                                            "nullable": false
                                        }
                                    }
                                },
                                {
                                    "column": {
                                        "name": "amount",
                                        "type": "DECIMAL(19,4)",
                                        "constraints": {
                                            "nullable": false
                                        }
                                    }
                                },
                                {
                                    "column": {
                                        "name": "description",
                                        "type": "TEXT"
                                    }
                                },
                                {
                                    "column": {
                                        "name": "fiscal_year",
                                        "type": "INT",
                                        "constraints": {
                                            "nullable": false
                                        }
                                    }
                                },
                                {
                                    "column": {
                                        "name": "fiscal_period",
                                        "type": "VARCHAR(20)"
                                    }
                                },
                                {
                                    "column": {
                                        "name": "company_name",
                                        "type": "VARCHAR(200)"
                                    }
                                },
                                {
                                    "column": {
                                        "name": "journal_entry_id",
                                        "type": "BIGINT"
                                    }
                                },
                                {
                                    "column": {
                                        "name": "is_posted",
                                        "type": "BOOLEAN",
                                        "defaultValueBoolean": false
                                    }
                                },
                                {
                                    "column": {
                                        "name": "posted_date",
                                        "type": "TIMESTAMP"
                                    }
                                },
                                {
                                    "column": {
                                        "name": "reference_number",
                                        "type": "VARCHAR(100)"
                                    }
                                },
                                {
                                    "column": {
                                        "name": "notes",
                                        "type": "TEXT"
                                    }
                                },
                                {
                                    "column": {
                                        "name": "created_at",
                                        "type": "TIMESTAMP",
                                        "defaultValueComputed": "CURRENT_TIMESTAMP"
                                    }
                                },
                                {
                                    "column": {
                                        "name": "updated_at",
                                        "type": "TIMESTAMP",
                                        "defaultValueComputed": "CURRENT_TIMESTAMP ON UPDATE CURRENT_TIMESTAMP"
                                    }
                                },
                                {
                                    "column": {
                                        "name": "created_by",
                                        "type": "VARCHAR(100)"
                                    }
                                },
                                {
                                    "column": {
                                        "name": "updated_by",
                                        "type": "VARCHAR(100)"
                                    }
                                }
                            ]
                        }
                    },
                    {
                        "addForeignKeyConstraint": {
                            "baseTableName": "equity_transactions",
                            "baseColumnNames": "equity_account_id",
                            "constraintName": "fk_equity_transactions_account",
                            "referencedTableName": "equity_accounts",
                            "referencedColumnNames": "equity_account_id"
                        }
                    },
                    {
                        "addForeignKeyConstraint": {
                            "baseTableName": "equity_transactions",
                            "baseColumnNames": "journal_entry_id",
                            "constraintName": "fk_equity_transactions_journal",
                            "referencedTableName": "journal_entries",
                            "referencedColumnNames": "journal_entry_id",
                            "onDelete": "SET NULL"
                        }
                    },
                    {
                        "createIndex": {
                            "indexName": "idx_equity_transactions_year",
                            "tableName": "equity_transactions",
                            "columns": [
                                {
                                    "column": {
                                        "name": "fiscal_year"
                                    }
                                }
                            ]
                        }
                    },
                    {
                        "createIndex": {
                            "indexName": "idx_equity_transactions_type",
                            "tableName": "equity_transactions",
                            "columns": [
                                {
                                    "column": {
                                        "name": "transaction_type"
                                    }
                                }
                            ]
                        }
                    },
                    {
                        "createIndex": {
                            "indexName": "idx_equity_transactions_account",
                            "tableName": "equity_transactions",
                            "columns": [
                                {
                                    "column": {
                                        "name": "equity_account_id"
                                    }
                                }
                            ]
                        }
                    }
                ]
            }
        }
    ]
}