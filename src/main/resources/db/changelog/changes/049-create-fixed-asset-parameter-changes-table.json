{
    "databaseChangeLog": [
        {
            "changeSet": {
                "id": "049-create-fixed-asset-parameter-changes-table",
                "author": "system",
                "changes": [
                    {
                        "createTable": {
                            "tableName": "fixed_asset_parameter_changes",
                            "columns": [
                                {
                                    "column": {
                                        "name": "change_id",
                                        "type": "BIGINT",
                                        "autoIncrement": true,
                                        "constraints": {
                                            "primaryKey": true,
                                            "nullable": false
                                        }
                                    }
                                },
                                {
                                    "column": {
                                        "name": "change_number",
                                        "type": "VARCHAR(50)",
                                        "constraints": {
                                            "unique": true,
                                            "nullable": false
                                        }
                                    }
                                },
                                {
                                    "column": {
                                        "name": "fixed_asset_id",
                                        "type": "BIGINT",
                                        "constraints": {
                                            "nullable": false
                                        }
                                    }
                                },
                                {
                                    "column": {
                                        "name": "change_date",
                                        "type": "DATE",
                                        "constraints": {
                                            "nullable": false
                                        }
                                    }
                                },
                                {
                                    "column": {
                                        "name": "change_type",
                                        "type": "VARCHAR(50)",
                                        "constraints": {
                                            "nullable": false
                                        }
                                    }
                                },
                                {
                                    "column": {
                                        "name": "change_reason",
                                        "type": "TEXT",
                                        "constraints": {
                                            "nullable": false
                                        }
                                    }
                                },
                                {
                                    "column": {
                                        "name": "old_gross_cost",
                                        "type": "DECIMAL(19,4)"
                                    }
                                },
                                {
                                    "column": {
                                        "name": "old_salvage_value",
                                        "type": "DECIMAL(19,4)"
                                    }
                                },
                                {
                                    "column": {
                                        "name": "old_useful_life_months",
                                        "type": "INT"
                                    }
                                },
                                {
                                    "column": {
                                        "name": "old_accumulated_depreciation",
                                        "type": "DECIMAL(19,4)"
                                    }
                                },
                                {
                                    "column": {
                                        "name": "new_gross_cost",
                                        "type": "DECIMAL(19,4)"
                                    }
                                },
                                {
                                    "column": {
                                        "name": "new_salvage_value",
                                        "type": "DECIMAL(19,4)"
                                    }
                                },
                                {
                                    "column": {
                                        "name": "new_useful_life_months",
                                        "type": "INT"
                                    }
                                },
                                {
                                    "column": {
                                        "name": "adjustment_amount",
                                        "type": "DECIMAL(19,4)"
                                    }
                                },
                                {
                                    "column": {
                                        "name": "journal_entry_id",
                                        "type": "BIGINT"
                                    }
                                },
                                {
                                    "column": {
                                        "name": "is_posted",
                                        "type": "BOOLEAN",
                                        "defaultValueBoolean": false
                                    }
                                },
                                {
                                    "column": {
                                        "name": "posted_date",
                                        "type": "TIMESTAMP"
                                    }
                                },
                                {
                                    "column": {
                                        "name": "posted_by",
                                        "type": "VARCHAR(100)"
                                    }
                                },
                                {
                                    "column": {
                                        "name": "created_at",
                                        "type": "TIMESTAMP",
                                        "defaultValueComputed": "CURRENT_TIMESTAMP"
                                    }
                                },
                                {
                                    "column": {
                                        "name": "created_by",
                                        "type": "VARCHAR(100)"
                                    }
                                }
                            ]
                        }
                    },
                    {
                        "addForeignKeyConstraint": {
                            "baseTableName": "fixed_asset_parameter_changes",
                            "baseColumnNames": "fixed_asset_id",
                            "constraintName": "fk_parameter_changes_asset",
                            "referencedTableName": "fixed_assets",
                            "referencedColumnNames": "fixed_asset_id"
                        }
                    },
                    {
                        "addForeignKeyConstraint": {
                            "baseTableName": "fixed_asset_parameter_changes",
                            "baseColumnNames": "journal_entry_id",
                            "constraintName": "fk_parameter_changes_journal",
                            "referencedTableName": "journal_entries",
                            "referencedColumnNames": "journal_entry_id",
                            "onDelete": "SET NULL"
                        }
                    }
                ]
            }
        }
    ]
}