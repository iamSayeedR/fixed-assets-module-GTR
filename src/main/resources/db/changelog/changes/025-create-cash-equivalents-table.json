{
    "databaseChangeLog": [
        {
            "changeSet": {
                "id": "018-create-cash-equivalents-table",
                "author": "system",
                "changes": [
                    {
                        "createTable": {
                            "tableName": "cash_equivalents",
                            "columns": [
                                {
                                    "column": {
                                        "name": "cash_equivalent_id",
                                        "type": "BIGINT",
                                        "autoIncrement": true,
                                        "constraints": {
                                            "primaryKey": true,
                                            "nullable": false
                                        }
                                    }
                                },
                                {
                                    "column": {
                                        "name": "description",
                                        "type": "VARCHAR(200)",
                                        "constraints": {
                                            "nullable": false
                                        }
                                    }
                                },
                                {
                                    "column": {
                                        "name": "price",
                                        "type": "DECIMAL(15,2)"
                                    }
                                },
                                {
                                    "column": {
                                        "name": "currency",
                                        "type": "VARCHAR(3)",
                                        "defaultValue": "USD"
                                    }
                                },
                                {
                                    "column": {
                                        "name": "group_name",
                                        "type": "VARCHAR(100)"
                                    }
                                },
                                {
                                    "column": {
                                        "name": "is_active",
                                        "type": "BOOLEAN",
                                        "defaultValueBoolean": true,
                                        "constraints": {
                                            "nullable": false
                                        }
                                    }
                                },
                                {
                                    "column": {
                                        "name": "created_at",
                                        "type": "TIMESTAMP",
                                        "defaultValueComputed": "CURRENT_TIMESTAMP",
                                        "constraints": {
                                            "nullable": false
                                        }
                                    }
                                },
                                {
                                    "column": {
                                        "name": "updated_at",
                                        "type": "TIMESTAMP",
                                        "defaultValueComputed": "CURRENT_TIMESTAMP",
                                        "constraints": {
                                            "nullable": false
                                        }
                                    }
                                }
                            ]
                        }
                    }
                ]
            }
        }
    ]
}